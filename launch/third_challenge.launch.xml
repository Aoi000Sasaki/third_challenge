<launch>
    <!-- param -->
    <arg name="goal_dist" default="1.0"/>
    <arg name="velocity" default="0.1"/>

    <!-- node -->
    <node pkg="third_challenge" exec="third_challenge_node" name="third_challenge_node"/>
        <param name="goal_dist" value="$(var goal_dist)"/>
        <param name="velocity" value="$(var velocity)"/>
    </node>
</launch>